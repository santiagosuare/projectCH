<h2>Lista de productos</h2>
  <div>
    {{#if productos}}
          <table class="table">
            <thead>
              <tr>
                <th scope="col">#</th>
                <th scope="col">Nombre</th>
                <th scope="col">Precio</th>
                <th scope="col">Thumbnail</th>
              </tr>
            </thead>
            <tbody>
              {{#each productos}}
                <tr>
                  <th scope="row">{{this.id}}</th>
                  <td>{{this.title}} </td>
                  <td>{{this.price}} </td>
                  <td><img src="{{this.thumbnail}}" alt="imagen del prod" /></td>
                </tr>
              {{/each}}
            </tbody>
          </table>
      {{else}}
          <div class="alert alert-danger" role="alert">
            No se encontraron datos
          </div>
    {{/if}}
  </div>