<% if(productosSql.length !== 0) { %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Nombre</th> <th>Precio</th> <th>Foto</th> </tr>
           <% for (const producto in productosSql ) { %>
            <tr>
                <td> <%= productos[producto].nombre %> </td>
                <td> $ <%= productos[producto].precio %> </td>
                <td> <img src=<%= productos[producto].foto %> alt=""> </td>
            </tr>
            <% } %>
        </table>
    </div>
<% } else{ %> 
   <h3 style="color: black">No se encontraron productos</h3>
<% }%>

